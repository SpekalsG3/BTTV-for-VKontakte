{
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "version": "1.0.5",
  "default_locale": "en",
  "description": "__MSG_extDesc__",
  "icons": {
    "64": "icons/64px.png",
    "128": "icons/128px.png",
    "512": "icons/512px.png"
  },
  "background": {
    "scripts": ["src/common.js"],
    "persistent": false
  },
  "content_scripts": [{
    "matches": ["*://vk.com/*"],
    "css": ["src/bttv_for_vk.css"],
    "js": ["src/index.js"]
  }],
  "permissions": [
    "storage"
  ],
  "web_accessible_resources": [
    "src/bttv_for_vk.js",
    "icons/mascot.png",
    "icons/settings_logo.png"
  ]
}