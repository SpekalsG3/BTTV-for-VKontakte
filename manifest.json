{
  "manifest_version": 2,
  "name": "BTTV for VKontakte",
  "version": "1.0",
  "description": "This is an unrelated version of BTTV for VKontakte",
  "icons": {
    "64": "icons/64px.png",
    "128": "icons/128px.png",
    "512": "icons/512px.png"
  },
  "background": {
    "scripts": ["common.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "64": "icons/64px.png",
      "128": "icons/128px.png",
      "512": "icons/512px.png"
    },
    "default_title": "BTTV for VKontakte",
    "default_popup": "popup/popup.html"
  },
  "content_scripts": [{
    "matches": ["*://*.vk.com/*"],
    "css": ["bttv_for_vk.css"],
    "js": ["index.js"]
  }],
  "permissions": [
    "storage",
    "activeTab"
  ],
  "web_accessible_resources": ["bttv_for_vk.js"]
}